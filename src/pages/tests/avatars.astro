---
import PersonPlaceholder from "../../components/placeholders/PersonPlaceholder.astro";
const avatars = new Array(26).fill("");
---

{
  avatars.map((_val, index) => {
    return <PersonPlaceholder iconId={index} class="mini-icon" />;
  })
}

<style>
  .mini-icon {
    width: 6rem;
    border-radius: 4rem;
    border: 3px solid var(--color-avatar-fg);
  }
</style>
